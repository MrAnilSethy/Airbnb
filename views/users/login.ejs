<% layout("/layouts/boilerplate")-%>
<div class="row mt-3">
    <h1 class="col-6 offset-3" >Login WanderLust</h1>
        <div class="col-4 offset-3">
    <form method="POST" class="needs-validation" action="/login" novalidate>
         <div class="mb-3">
            <label for="username" class="form-label"><b>Username</b></label>
            <input name="username" type="text" id="username" class="form-control" required />
        </div>
        <div class="mb-3">
            <label for="password" class="form-label"><b>Password</b></label>
            <input name="password" class="form-control" type="password" id="password" required />
        </div>
         <button class="btn btn-success">Login</button>
        </div>
    </form>
    </div>
    </div>